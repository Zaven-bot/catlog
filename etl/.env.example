# CatLog ETL Pipeline Environment Variables

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/catlog
DB_HOST=localhost
DB_PORT=5432
DB_NAME=catlog
DB_USER=postgres
DB_PASSWORD=your_password_here

# BigQuery Configuration (Stage 2 - Cloud Warehouse Sync)
ENABLE_CLOUD_SYNC=false
GCP_PROJECT_ID=your-gcp-project-id
BIGQUERY_DATASET=catlog_anime_data
BIGQUERY_TABLE=processed_anime
BIGQUERY_LOCATION=US
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/service-account-key.json

# Optional: Override default ETL settings
# JIKAN_RATE_LIMIT_DELAY=1.0
# JIKAN_MAX_RETRIES=3
# ETL_BATCH_SIZE=100
# ETL_MAX_PAGES=10